#!/bin/bash
dir=${1:-*}

echo path,server,client,protocol,Gbits/s
ls $dir/iperf_tcp_srv_mocr4pcc02u* | while read file; do
    res=$(tail -1 $file);res=${res%% Gbits*};
    clt=${file%%.log}
    clt=${clt##*_}
    srv=${file##*srv_}
    srv=${srv%%_*}
    echo $file,$srv,$clt,tcp,${res##* };
done


